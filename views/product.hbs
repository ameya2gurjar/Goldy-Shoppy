<div class="container">
  <div class="columns">
    <div class="column">
      {{#each product.images}}
        <img src="/upload/view/{{this}}"/>
      {{/each}}
    </div>

    <div class="column">
      <h1 class="title">{{product.name}}</h1>
      <p>{{posted_by.displayName}}</p>
      <p id="posted_at">{{product.posted_at}}</p>
      <p>{{product.description}}</p>
    </div>
  </div>
</div>

<script> document.getElementById("demo").innerHTML = time(2); </script>
